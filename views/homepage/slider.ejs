<%for (let i of data.workData.objects) {%>
    <div onclick="workOpener('<%=i.slug%>')" class="cursor-pointer flex flex-shrink-0 relative w-full sm:w-auto">
        <img style="max-height: 600px;min-height: 600px;" src="<%=i.metadata.image.imgix_url%>"
            alt="black chair and white table" class="object-cover object-center w-full" />
        <div class="bg-gray-800 bg-opacity-30 absolute w-full h-full p-6">
            <h2 class="lg:text-xl leading-4 text-base lg:leading-5 text-white ">
                <%=i.metadata.location%>
            </h2>
            <div class="flex h-full items-end pb-6">
                <h3 class="text-xl lg:text-2xl font-semibold leading-5 lg:leading-6 text-white">
                    <%=i.title%>
                </h3>
            </div>
        </div>
    </div>

    <%}%>